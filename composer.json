{
    "name": "code4interactive/notifications",
    "description": "Notification package for Laravel",
    "keywords": [
        "notifications",
        "laravel"
    ],
    "homepage": "https://github.com/code4interactive/notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Bartczak",
            "email": "artur.bartczak@code4.pl"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "illuminate/support" : "~5.0",
        "illuminate/session" : "~5.0",
        "illuminate/contracts" : "~5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Code4\\Notifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code4\\Notifications\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
